<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Regression Tests" parallel="methods" thread-count="1">
    <listeners>
        <listener class-name="listeners.TestListener"/>
    </listeners>
    <test name="Login">
        <classes>
            <class name="testCases.LoginTest"/>
        </classes>
    </test>
    <test name="Contact Us">
        <parameter name="jsonFilePath" value="src/test/java/resources/testData/contactFormData.json" />
        <classes>
            <class name="testCases.ContactFormTest"/>
        </classes>
    </test>
</suite>